{
  "name": "mytube",
  "version": "0.1.0",
  "author": "doublelayer",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_ENV=dev&& node server",
    "client-dev": "cd client && yarn dev",
    "start-dev": "concurrently --kill-others \"yarn dev\" \"yarn client-dev\"",
    "postinstall": "cd client && npm install",
    "prestart": "cd client && yarn build",
    "start": "set NODE_ENV=prod node server",
    "server": "set NODE_ENV=dev&& node server",
    "installclient": "cd client && pnpm install",
    "installserver": "cd server && pnpm install",
    "install": "\"installclient\" \"installserver\""
  },
  "dependencies": {},
  "devDependencies": {}
}
